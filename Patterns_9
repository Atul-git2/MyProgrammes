Scanner sc = new Scanner(System.in);
		System.out.println("Enter the range of the table");
		int s = sc.nextInt();
		System.out.println("Enter the table that you want to print");
		int num = sc.nextInt();
		for (int i = 1; i <= 10; i++) {
			for (int j = 1; j <= 20; j++) {
				if (j == num) {
					System.out.println(i * j + " ");
				}
				else {
					System.out.println("Not in range");
				}
